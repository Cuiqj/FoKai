			<pdf:tr height="10.65"><pdf:td width="177">{{! 空白行 }}</pdf:td></pdf:tr>
			<pdf:tr height="10.65"><pdf:td width="177">{{! 空白行 }}</pdf:td></pdf:tr>
			<pdf:tr height="10.65"><pdf:td width="177">{{! 空白行 }}</pdf:td></pdf:tr>
			<pdf:tr height="10.65"><pdf:td width="177">{{! 空白行 }}</pdf:td></pdf:tr>
            <pdf:tr height="10.65"><pdf:td width="177">{{! 空白行 }}</pdf:td></pdf:tr>
            <pdf:tr height="10.65"><pdf:td width="177">{{! 空白行 }}</pdf:td></pdf:tr>
			<pdf:tr height="10.65">
                <pdf:td width="177">
                    <pdf:text point="mid-left:3.5,6">被询问人签名并按印：</pdf:text>
                </pdf:td>
<!--                <pdf:td width="177" textAlignment="left">被询问人签名并按印：</pdf:td>-->
			</pdf:tr>
			<pdf:tr height="10.65">
				<pdf:td width="177">
					<pdf:text point="mid-left:3.5,6">询问人签名：</pdf:text>
					<pdf:text point="mid-left:72,6">证件号码：</pdf:text>
                    <pdf:text point="mid-left:91,6"static="NO">{{ caseInquire.inquirernum  }}</pdf:text>
				</pdf:td>
			</pdf:tr>
			<pdf:tr height="10.65">
				<pdf:td width="177">
					<pdf:text point="mid-left:3.5,6">记录人签名：</pdf:text>
					<pdf:text point="mid-left:72,6">证件号码：</pdf:text>
                    <pdf:text point="mid-left:91,6" static="NO">{{ caseInquire.recordernum }}</pdf:text>
				</pdf:td>
			</pdf:tr>
			<pdf:tr height="10.65">
				<pdf:td width="177">
					<pdf:text point="mid-left:132,4.5">共</pdf:text>
					<pdf:text point="mid-left:140,4.5" static="NO">{{ page.pageCount  }}</pdf:text>
					<pdf:text point="mid-left:147,4.5">页第</pdf:text>
					<pdf:text point="mid-left:160,4.5" static="NO">{{ page.pageNum  }}</pdf:text>
					<pdf:text point="mid-left:167,4.5">页</pdf:text>
				</pdf:td>
			</pdf:tr>
		</pdf:table>
		<pdf:text point="16.5,285">（本页填写不下的内容，可另附纸。）</pdf:text>
