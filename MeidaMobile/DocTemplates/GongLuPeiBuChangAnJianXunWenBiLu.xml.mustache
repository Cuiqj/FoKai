				<pdf:td width="177">
					<pdf:text point="mid-left:3.5,6">时间：</pdf:text>
                    <pdf:text point="mid-left:18,6" static="NO">{{ caseInquire.date }}</pdf:text>
					<pdf:text point="mid-left:80,6">地点：</pdf:text>
                    <pdf:text point="mid-left:93,6" maxWidth = "81" static="NO" >{{ caseInquire.place }}</pdf:text>
				</pdf:td>
			</pdf:tr>
			<pdf:tr height="10.7">
				<pdf:td width="177">
					<pdf:text point="mid-left:3.5,6">询问人：</pdf:text>
                    <pdf:text point="mid-left:21,6" static="NO">{{ caseInquire.inquirerName }}</pdf:text>
					<pdf:text point="mid-left:80,6">记录人：</pdf:text>
                    <pdf:text point="mid-left:100,6" static="NO">{{ caseInquire.recorderName }}</pdf:text>
				</pdf:td>
			</pdf:tr>
			<pdf:tr height="10.65">
				<pdf:td width="177">
					<pdf:text point="mid-left:3.5,6">被询问人：</pdf:text>
                    <pdf:text point="mid-left:26,6" static="NO">{{ caseInquire.answererName }}</pdf:text>
					<pdf:text point="mid-left:48,6">性别：</pdf:text>
                    <pdf:text point="mid-left:59,6" static="NO">{{ caseInquire.answererSex }}</pdf:text>
					<pdf:text point="mid-left:77,6">年龄：</pdf:text>
                    <pdf:text point="mid-left:88,6" static="NO">{{ caseInquire.answererAge }}</pdf:text>
					<pdf:text point="mid-left:105,6">与案件关系：</pdf:text>
                    <pdf:text point="mid-left:128,6" static="NO">{{ caseInquire.answererRelation }}</pdf:text>
				</pdf:td>
			</pdf:tr>
			<pdf:tr height="10.65">
				<pdf:td width="177">
					<pdf:text point="mid-left:3.5,6">工作单位和职务：</pdf:text>
                    <pdf:text point="mid-left:40,6" maxWidth="75" static="NO">{{ caseInquire.answererOrgDuty }}</pdf:text>
					<pdf:text point="mid-left:116,6">电话：</pdf:text>
                    <pdf:text point="mid-left:128,6" static="NO">{{ caseInquire.answererPhoneNum }}</pdf:text>
				</pdf:td>
			</pdf:tr>
			<pdf:tr height="10.65">
				<pdf:td width="177">
					<pdf:text point="mid-left:3.5,6">地址：</pdf:text>
                    <pdf:text point="mid-left:18,6" maxWidth="96" static="NO">{{ caseInquire.answererAddress }}</pdf:text>
					<pdf:text point="mid-left:116,6">邮编：</pdf:text>
                    <pdf:text point="mid-left:128,6" static="NO">{{ caseInquire.answererPostalCode }}</pdf:text>
				</pdf:td>
			</pdf:tr>
			<pdf:tr height="10.65"><pdf:td width="177">
                <pdf:paragraph rect="{3.5,0},{170,240}" lineHeight="10.3" static="NO">
                    {{ caseInquire.inquireNote }}
                </pdf:paragraph>
            </pdf:td></pdf:tr>
			<pdf:tr height="10.65"><pdf:td width="177">{{! 空白行 }}</pdf:td></pdf:tr>
