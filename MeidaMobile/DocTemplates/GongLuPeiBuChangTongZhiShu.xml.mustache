        <pdf:text point="23,217">书之日起15日内持本通知书到</pdf:text>
        
<!--        <pdf:line fromPoint="91.5,222" toPoint="130,222" lineWidth="0.5"/>-->

        <pdf:table rect="{92.5,213.5},{37.5,10}" borderWidth="0.1" lineWidth="0.1" padding="0.1">
