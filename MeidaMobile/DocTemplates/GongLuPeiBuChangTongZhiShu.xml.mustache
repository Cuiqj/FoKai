                <pdf:td width="37.5" lineHeight="4" fontSize = "10" static="NO">{{ paymentPlace }}</pdf:td>
            </pdf:tr>
        </pdf:table>
        
<!--        <pdf:paragraph rect="{92,214},{40,9}" indent="head:0" lineHeight="4"  static="NO" fontSize="10">{{ paymentPlace }}</pdf:paragraph>-->
